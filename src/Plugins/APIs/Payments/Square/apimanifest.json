{
    "applicationName": "Square Plugin",
    "description": "This plugin accesses Square API to retrieve financial data and manage payments.",
    "publisher": {
        "name": "Tyler",
        "contactEmail": "contact.tylerkendrick@gmail.com"
    },
    "apiDependencies": {
        "square": {
            "apiDescriptionUrl": "https://raw.githubusercontent.com/square/square-api-specifications/main/api.json",
            "requests": [
                {
                    "method": "GET",
                    "uriTemplate": "/v2/accounts"
                },
                {
                    "method": "POST",
                    "uriTemplate": "/v2/transactions"
                },
                {
                    "method": "GET",
                    "uriTemplate": "/v2/transactions/{transaction_id}"
                },
                {
                    "method": "POST",
                    "uriTemplate": "/v2/payments"
                },
                {
                    "method": "GET",
                    "uriTemplate": "/v2/payments/{payment_id}"
                },
                {
                    "method": "POST",
                    "uriTemplate": "/v2/refunds"
                }
            ]
        }
    }
}
