.{
  "version": "1.0.0",
  "clients": {
    "IFTTTClient": {
      "language": "CSharp",
      "structuredMimeTypes": [
        "application/json",
        "text/plain;q=0.9",
        "application/x-www-form-urlencoded;q=0.2",
        "multipart/form-data;q=0.1"
      ],
      "clientNamespaceName": "IFTTT.SDK",
      "usesBackingStore": false,
      "includeAdditionalData": true,
      "excludeBackwardCompatible": false,
      "disabledValidationRules": [],
      "descriptionLocation": "https://raw.githubusercontent.com/IFTTT/service-api/main/service-api.yaml",
      "includePatterns": [
        "/",
        "/ifttt",
        "/ifttt/v1",
        "/ifttt/v1/actions",
        "/ifttt/v1/actions/{stepSlug}",
        "/ifttt/v1/actions/{stepSlug}/fields",
        "/ifttt/v1/actions/{stepSlug}/fields/{stepFieldSlug}",
        "/ifttt/v1/actions/{stepSlug}/fields/{stepFieldSlug}/options",
        "/ifttt/v1/queries",
        "/ifttt/v1/queries/{stepSlug}",
        "/ifttt/v1/queries/{stepSlug}/fields",
        "/ifttt/v1/queries/{stepSlug}/fields/{stepFieldSlug}",
        "/ifttt/v1/queries/{stepSlug}/fields/{stepFieldSlug}/options",
        "/ifttt/v1/status",
        "/ifttt/v1/test",
        "/ifttt/v1/test/setup",
        "/ifttt/v1/triggers",
        "/ifttt/v1/triggers/{stepSlug}",
        "/ifttt/v1/triggers/{stepSlug}/fields",
        "/ifttt/v1/triggers/{stepSlug}/fields/validate",
        "/ifttt/v1/triggers/{stepSlug}/fields/{stepFieldSlug}",
        "/ifttt/v1/triggers/{stepSlug}/fields/{stepFieldSlug}/options",
        "/ifttt/v1/triggers/{stepSlug}/fields/{stepFieldSlug}/validate",
        "/ifttt/v1/user",
        "/ifttt/v1/user/info"
      ],
      "excludePatterns": [],
      "outputPath": "./src/Plugins/APIs/IFTTT"
    }
  },
  "plugins": {}
}